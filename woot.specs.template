-melf_i386 %{shared:-shared -Bsymbolic} %{!shared:-lc} -L<<ROOT_DIR>>/libc -rpath-link=<<ROOT_DIR>>/libc -L<<ROOT_DIR>>/libwoot -rpath-link=<<ROOT_DIR>>/libwoot -L<<ROOT_DIR>>/zlib/lib -rpath-link=<<ROOT_DIR>>/zlib/lib -L<<ROOT_DIR>>/libpng/lib -rpath-link=<<ROOT_DIR>>/libpng/lib -L<<ROOT_DIR>>/libfreetype/lib -rpath-link=<<ROOT_DIR>>/libfreetype/lib -L<<ROOT_DIR>>/libgmp/lib -rpath-link=<<ROOT_DIR>>/libgmp/lib

